{
  "name": "common-tg-service",
  "version": "1.1.22",
  "description": "Common Telegram service for NestJS applications",
  "author": "",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "keywords": [
    "nestjs",
    "telegram",
    "common",
    "service"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build && tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "start": "cross-env NODE_OPTIONS=--expose-gc nest start",
    "start:prod": "cross-env NODE_OPTIONS=--expose-gc node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:webpack": "node dist/index.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "webpack-dev": "webpack --watch --mode development",
    "webpack": "webpack --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "upload-cloudinary": "node upload-build.js",
    "build:webpack": "npm i --f && npm run webpack -- --config ./webpack-prod.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@types/multer": "^1.4.13",
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.7.0",
    "https": "^1.0.0",
    "imap": "^0.8.19",
    "mongoose": "^8.16.0",
    "node-fetch": "^2.7.0",
    "rxjs": "^7.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "telegram": "^2.26.22"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.3",
    "@types/express": "5.0.3",
    "@types/imap": "0.8.42",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.3",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "cross-env": "^7.0.3",
    "eslint": "9.29.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.0",
    "jest": "30.0.2",
    "prettier": "3.6.0",
    "source-map-support": "0.5.21",
    "supertest": "7.1.1",
    "ts-jest": "29.4.0",
    "ts-loader": "9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3",
    "webpack": "5.99.9",
    "webpack-cli": "6.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
